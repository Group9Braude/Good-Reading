      <Text layoutX="257.0" layoutY="266.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Book file format:" />
      <Button layoutX="267.0" layoutY="313.0" mnemonicParsing="false" onAction="#onGenerate" text="Generate book file" />
      <Button layoutX="292.0" layoutY="347.0" mnemonicParsing="false" onAction="#onLogOut" text="Log out" />
      <Button layoutX="251.0" layoutY="212.0" mnemonicParsing="false" onAction="#popUpCredit" text="Enter a new credit card" />
   </children>
</AnchorPane>
